#include <iostream>

using namespace std;

int main()
{
  float duration;
  
  cout << "How many hours have you worn the mask?\n";
  cin >> duration;
  
  if (duration >= 4.0) {
      cout << "Change your mask to a new one,\n"
              "don't forget to wash your hands!";
  }
  else {
      cout << "You can keep wearing your mask.\n"
              "Wear it properly, make sure it's not wet or dirty.";
  }
  
  return 0;
  }
